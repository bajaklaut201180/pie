body{
    
}
#wrapper{
    
}
.vertical-center{
    min-height: 100%;  /* if borwser NOT support vh unit */
    min-height: 100vh; 
    display: flex;
    align-items: center;
}
.form-login{
    max-width: 50%;
    margin: 0px auto;
}
#main-navigasi{
	background: #011C40;
}
#main-navigasi .nav .open>a:focus, #main-navigasi .nav .open>a:hover{
	background: #C30D23;
}
#main-navigasi .nav>li>a:focus, #main-navigasi .nav>li>a:hover, #main-navigasi .sidebar ul li a.active{
	background: #C30D23;
	color: #fff;
}
#page-wrapper select.form-control.input-sm, #page-wrapper .pagination>li:first-child>a, #page-wrapper .pagination>li:first-child>span, #page-wrapper .pagination>li:last-child>a, #page-wrapper .pagination>li:last-child>span{
	background: #601986; 
	color: #fff;
}
#page-wrapper .panel-default{
	border-color: #601986 !important;
}
.btn-submit{
	background: #fff;
	color: #000;
}
button.btn-submit:hover{
    background: #601986 !important;
    color: #fff;
}
.black-flag{
    background: black;
    width: 10px;
    height: 10px;
    display: block;
    margin: 0px 10px;
    display: inline-block;  
}
.green-flag{
    background: green;
    width: 10px;
    height: 10px;
    display: block;
    margin: 0px 10px;
    display: inline-block;
}
.red-flag{
    background: #f00;
    width: 10px;
    height: 10px;
    display: block;
    margin: 0px 10px;
    display: inline-block;
}
.btn-upload{
    width: 100%;
}
input[type=file]{
    margin: 0px auto;
}
.navbar-top-links .fa{
	color:#fff;
}
